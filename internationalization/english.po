#: main.cpp:191
msgid "Syntaxe:"
msgstr "Syntax:"

#: main.cpp:193
msgid ""
" [options...] --read [\"fichier\" \"fichier\"...] --hosts [\"hébergeur\" "
"\"hébergeur\"...] [2>liens.txt]"
msgstr ""
" [options...] --read [\"file\" \"file\"...] --hosts [\"host\" "
"\"host\"...] [2>links.txt]"

#: main.cpp:195
msgid "Options:"
msgstr "Options:"

#: main.cpp:196
msgid "\t--login <nom> --password <password>"
msgstr "\t--login <username> --password <password>"

#: main.cpp:197
msgid "\t--view\t\tSimulation d'upload (vérification des paramètres)."
msgstr "\t--view\t\tSimulation of the upload (verification of the parameters)."

#: main.cpp:198
msgid "\t--hosts\t\tSpécifier une liste personnelle d'hébergeurs."
msgstr "\t--hosts\t\tSpecify your own list of hosts."

#: main.cpp:199
msgid "\t--output\tEnregistre les liens dans le fichier choisi."
msgstr "\t[2>liens.txt]\tRedirect links from stderr to a specific file."

#: main.cpp:200
msgid "\t--read\t\tEnvoyer un ou plusieurs fichiers."
msgstr "\t--read\t\tSend one or more files."

#: main.cpp:201
msgid "\t[2>liens.txt]\tRedirige stderr pour avoir les liens uniquement."
msgstr "\t[2>liens.txt]\tRedirect links from stderr to a specific file."

